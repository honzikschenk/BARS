<mujoco model="bars">
  <compiler angle="degree" coordinate="local" inertiafromgeom="true" meshdir="models/" />

  <visual>
    <headlight ambient=".7 .7 .7" diffuse="0.3 0.3 0.3" specular=".8 .8 .8" />
    <quality shadowsize="8192" />
  </visual>

  <default>
    <geom type="mesh" xyaxes="1 0 0 0 0 1" />
  </default>

  <asset>
    <mesh name="Pelvis" file="Pelvis.stl" scale="0.001 0.001 0.001" />
    <mesh name="LeftThigh" file="LeftThigh.stl" scale="0.001 0.001 0.001" />
    <mesh name="LeftCalf" file="LeftCalf.stl" scale="0.001 0.001 0.001" />
    <mesh name="LeftFoot" file="LeftFoot.stl" scale="0.001 0.001 0.001" />
    <mesh name="LeftRollHip" file="LeftRollHip.stl" scale="0.001 0.001 0.001" />
    <mesh name="LeftYawHip" file="LeftYawHip.stl" scale="0.001 0.001 0.001" />
    <mesh name="RightThigh" file="RightThigh.stl" scale="0.001 0.001 0.001" />
    <mesh name="RightCalf" file="RightCalf.stl" scale="0.001 0.001 0.001" />
    <mesh name="RightFoot" file="RightFoot.stl" scale="0.001 0.001 0.001" />
    <mesh name="RightRollHip" file="RightRollHip.stl" scale="0.001 0.001 0.001" />
    <mesh name="RightYawHip" file="RightYawHip.stl" scale="0.001 0.001 0.001" />

    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>

  <worldbody>
    <geom name="Floor" type="plane" size="3 3 .001" axisangle="1 0 0 0" material="grid"/>

    <body name="Pelvis" axisangle="1 0 0 270" pos="0 0 0.8">
      <freejoint />
      <geom mesh="Pelvis" />
      <body name="LeftRollHip">
        <joint name="left_pitch_hip" type="hinge" axis="1 0 0" pos="0 0.051 -0.015" />
        <geom mesh="LeftRollHip" />
        <body name="LeftYawHip">
          <joint name="left_roll_hip" type="hinge" axis="0 0 1" pos="0.1 0.077 0" />
          <geom mesh="LeftYawHip" />
          <body name="LeftThigh">
            <joint name="left_yaw_hip" type="hinge" axis="0 1 0" pos="0.099 0 -0.0669" />
            <geom mesh="LeftThigh" />
            <body name="LeftCalf">
              <joint name="left_knee" type="hinge" axis="1 0 0" pos="0 0.3437 -0.0576" />
              <geom mesh="LeftCalf" />
              <body name="LeftFoot" pos="0.092 0.5067 -0.0528" axisangle="0 1 0 90">
                <joint name="left_ankle" type="hinge" axis="0 0 -1" pos="0 0 0" />
                <geom mesh="LeftFoot" />
              </body>
            </body>
          </body>
        </body>
      </body>
      <body name="RightRollHip">
        <joint name="right_pitch_hip" type="hinge" axis="1 0 0" pos="0 0.051 -0.015" />
        <geom mesh="RightRollHip" />
        <body name="RightYawHip">
          <joint name="right_roll_hip" type="hinge" axis="0 0 1" pos="-0.1 0.077 0" />
          <geom mesh="RightYawHip" />
          <body name="RightThigh">
            <joint name="right_yaw_hip" type="hinge" axis="0 1 0" pos="-0.099 0 -0.0669" />
            <geom mesh="RightThigh" />
            <body name="RightCalf">
              <joint name="right_knee" type="hinge" axis="1 0 0" pos="0 0.3437 -0.0576" />
              <geom mesh="RightCalf" />
              <body name="RightFoot" pos="-0.0575 0.5067 -0.0528" axisangle="0 1 0 90">
                <joint name="right_ankle" type="hinge" axis="0 0 -1" pos="0 0 0" />
                <geom mesh="RightFoot" />
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <position name="left_ankle" joint="left_ankle" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="left_knee" joint="left_knee" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1" kv="0.1" />
    <position name="left_yaw_hip" joint="left_yaw_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="left_roll_hip" joint="left_roll_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="left_pitch_hip" joint="left_pitch_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708"
      kp="1"
      kv="0.1" />

    <position name="right_ankle" joint="right_ankle" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="right_knee" joint="right_knee" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="right_yaw_hip" joint="right_yaw_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708" kp="1"
      kv="0.1" />
    <position name="right_roll_hip" joint="right_roll_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708"
      kp="1"
      kv="0.1" />
    <position name="right_pitch_hip" joint="right_pitch_hip" ctrllimited="true" ctrlrange="-1.5708 1.5708"
      kp="1"
      kv="0.1" />
  </actuator>

  <contact>
    <!-- <exclude body1="Floor" body2="LeftFoot" /> -->
    <exclude body1="LeftFoot" body2="LeftCalf" />
    <exclude body1="LeftCalf" body2="LeftThigh" />
    <exclude body1="LeftThigh" body2="LeftYawHip" />
    <exclude body1="LeftYawHip" body2="LeftRollHip" />
    <exclude body1="LeftRollHip" body2="Pelvis" />

    <!-- <exclude body1="Floor" body2="RightFoot" /> -->
    <exclude body1="RightFoot" body2="RightCalf" />
    <exclude body1="RightCalf" body2="RightThigh" />
    <exclude body1="RightThigh" body2="RightYawHip" />
    <exclude body1="RightYawHip" body2="RightRollHip" />
    <exclude body1="RightRollHip" body2="Pelvis" />
  </contact>
</mujoco>
